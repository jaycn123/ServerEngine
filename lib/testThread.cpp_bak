#include <iostream>  
#include "ThreadPool.h"  
#include <unistd.h>
  
  
void Test(int i)  
{  
    printf("test Thread %d time\n",i);  
    sleep(2);
}  
  
int main()  
{  
    fivestar::ThreadPool threadPool;  
    threadPool.setMaxQueueSize(1000);  
    threadPool.start(4);  
  
    for(int i = 0;i < 20;++i)  
    {  
        auto task = std::bind(Test,i);  
        threadPool.run(task);  
    }  
  
    return 0;  
}  
